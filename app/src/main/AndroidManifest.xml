    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_screen_activity" />
        <activity android:name=".Games_list_activity" />
        <activity android:name=".Game_Information_activity" />
        <activity android:name=".Register_activity" />
        <activity android:name=".Contact_screen_activity"></activity>
    </application>

</manifest>